 <gui_config>
    <atlas file="gui/atlas2.png"/>
	<fonts>
	
	</fonts>
	<general_sfx>
		<sfx name="standardClick" path="UI/click.wav"/>
		<sfx name="standardHover" path="UI/mouse_enter.wav"/>
	</general_sfx>
	<gui_element_templates>
		<buttons>
			<button_type_1>
				<up x="0" y="0" w="120" h="40"/>
				<hover x="0" y="40" w="120" h="40"/>
				<down x="0" y="80" w="120" h="40" offset="10"/>
				<bounds x="0" y="0" w="240" h="80"/>
				<font name="digiffiti" size="32" path="fonts/digiffiti.ttf"/>
				<font_color r="255" g="83" b="13" a="0"/>
				<sfx click="standardClick" hover="standardHover"/>
        <move_text_down amount ="20"/>
			</button_type_1>
			<button_type_2>
				<up x="67" y="125" w="12" h="13"/>
				<hover x="67" y="141" w="12" h="13"/>
				<down x="67" y="157" w="12" h="13"/>
				<bounds x="0" y="0" w="32" h="32"/>
				<!-- On this example we use the same font, but only loads once!
					 checkfont function on gui, detects if we demands the same
					 font with exactly the same size and skips.
					 And prints with other color -->
				<font name="digiffiti" size="32" path="fonts/digiffiti.ttf"/>
				<font_color r="255" g="255" b="255" a="0"/>
        <move_text_down amount ="20"/>
			</button_type_2>
		</buttons>
		<checkboxes>
			<checkbox_type_1>
				<up x="16" y="125" w="16" h="16"/>
				<hover x="0" y="125" w="16" h="16"/>
				<down x="32" y="125" w="16" h="16"/>
				<upCheck x="16" y="141" w="16" h="16"/>
				<hoverCheck x="0" y="141" w="16" h="16"/>
				<downCheck x="32" y="141" w="16" h="16"/>
				<bounds x="0" y="0" w="32" h="32"/>
				<font name="digiffiti" size="32" path="fonts/digiffiti.ttf"/>
				<font_color r="255" g="255" b="255" a="0"/>
				<sfx click="standardClick" hover="standardHover"/>
			</checkbox_type_1>
		</checkboxes>
		<windows>
			<window_type_1>
				<up x="0" y="0" w="0" h="0"/>
				<hover x="0" y="0" w="0" h="0"/>
				<down x="0" y="0" w="0" h="0"/>
			</window_type_1>
		</windows>
	</gui_element_templates>
	<button_up file="gui/UI-DialogBox-Button-Up.png"/>
	<button_down file="gui/UI-DialogBox-Button-Down.png"/>
	<button_hover file="gui/UI-DialogBox-Button-Highlight.png"/>
	<checkbox_up file="gui/UI-CheckBox-Up.png"/>
	<checkbox_down file="gui/UI-CheckBox-Down.png"/>
	<checkbox_highlight file="gui/UI-CheckBox-Highlight.png"/>
	<checkbox_check file="gui/UI-CheckBox-Check.png"/>
	<checkbox_check_locked file="gui/UI-CheckBox-Check-Disabled.png"/>

  </gui_config>